{
  "version": 2,
  "buildCommand": "echo 'Backend-only deployment - skipping frontend build'",
  "outputDirectory": ".",
  "functions": {
    "index.cjs": {
      "runtime": "nodejs20.x",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.cjs"
    }
  ]
}
